{
    "version": 2,
    "buildCommand": "npm run build:frontend",
    "outputDirectory": "dist/apps/frontend",
    "installCommand": "npm install && npm run prisma-generate",
    "framework": "nextjs",
    "rootDirectory": "apps/frontend",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ]
}